    <script>
        function sendBookingWhatsApp(event) {
            event.preventDefault();
            
            const firstName = document.getElementById('firstName').value;
            const lastName = document.getElementById('lastName').value;
            const email = document.getElementById('email').value;
            const phone = document.getElementById('phone').value || 'Not provided';
            const travelDate = document.getElementById('travelDate').value;
            const groupSize = document.getElementById('groupSize').value;
            const accommodation = document.getElementById('accommodationLevel').value;
            const specialRequests = document.getElementById('specialRequests').value || 'None';
            
            // Get selected interests
            const interests = [];
            if (document.getElementById('photography').checked) interests.push('Wildlife photography focus');
            if (document.getElementById('birdwatching').checked) interests.push('Extended bird watching');
            if (document.getElementById('conservation').checked) interests.push('Conservation education focus');
            if (document.getElementById('canopy').checked) interests.push('Extended canopy experience');
            
            const interestsText = interests.length > 0 ? interests.join(', ') : 'General nature tour';
            
            const message = `ðŸ’ RWANDA GOLDEN MONKEY & NATURE TOUR BOOKING REQUEST

ðŸ“ GUEST DETAILS:
Name: ${firstName} ${lastName}
Email: ${email}
Phone: ${phone}

ðŸ—“ï¸ TRAVEL DETAILS:
Preferred Date: ${travelDate}
Group Size: ${groupSize} people
Lodge Preference: ${accommodation}

ðŸŒ¿ NATURE INTERESTS:
${interestsText}

ðŸ’¬ SPECIAL REQUESTS:
${specialRequests}

Please provide a detailed quote and availability for this Rwanda Golden Monkey & Nature Tour package. Thank you!`;
            
            const whatsappURL = `https://wa.me/256781232166?text=${encodeURIComponent(message)}`;
            window.open(whatsappURL, '_blank');
        }
        
        // Fade in elements on scroll
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver(function(entries) {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        document.addEventListener('DOMContentLoaded', function() {
            const fadeElements = document.querySelectorAll('.day-card, .highlight-box, .wildlife-facts, .canopy-experience');
            fadeElements.forEach(el => {
                el.style.opacity = '0';
                el.style.transform = 'translateY(30px)';
                el.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
                observer.observe(el);
            });
            
            // Set minimum date to tomorrow
            const dateInput = document.getElementById('travelDate');
            const tomorrow = new Date();
            tomorrow.setDate(tomorrow.getDate() + 1);
            dateInput.min = tomorrow.toISOString().split('T')[0];
        });
    </script>
